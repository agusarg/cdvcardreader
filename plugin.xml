<?xml version="1.0" encoding="UTF-8"?>
    <plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
            id="com.cdvcardreader" version="0.2.3">
        <name>CDVCardReader</name>
        <description>Plugin que lee tarjeta magnetica</description>
        <license>Apache 2.0</license>
        <keywords>cordova,device</keywords>

        <platform name="ios">
            <config-file target="config.xml" parent="/*">
                <feature name="CDVCardReader">
                    <param name="ios-package" value="CDVCardReader"/>
                </feature>
            </config-file>
            <header-file src="src/ios/CDVCardReader.h" />
            <source-file src="src/ios/CDVCardReader.m" />
        </platform>
    </plugin>